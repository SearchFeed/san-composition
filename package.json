{
  "name": "san-composition",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "private": true,
  "description": "San Composition API",
  "scripts": {
    "dev": "cross-env NODE_ENV=development san serve --config ./example/san.config.js",
    "build": "yarn run clear && yarn run rollup",
    "rollup": "rollup -c",
    "clear": "rimraf ./dist",
    "test": "jest --coverage",
    "prepublish": "yarn run build",
    "lint": "eslint --ignore-path .eslintignore ./",
    "lint:fix": "eslint --ignore-path .eslintignore ./ --fix"
  },
  "keywords": [
    "San",
    "San Composition API"
  ],
  "author": "jinzhan <jinzhan@baidu.com>",
  "license": "MIT",
  "devDependencies": {
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^7.0.2",
    "postcss-import": "^14.0.2",
    "postcss-url": "^10.1.3",
    "san-cli": "^4.0.0",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "jest": "^27.0.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.1",
    "axios": "^0.21.1",
    "san": "^3.10.0",
    "husky": "^4.0.0",
    "lint-staged": "^9.2.5",
    "@ecomfe/eslint-config": "^3.2.0",
    "eslint": "^6.8.0",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jest": "^23.8.2"
  },
  "peerDependencies": {
    "san": "^3.10.0"
  },
  "lint-staged": {
    "!(example|dist|output|__mocks__)/**/*.js": "eslint"
  },
  "husky": {
      "hooks": {
          "pre-commit": "lint-staged && yarn test"
      }
  }
}
